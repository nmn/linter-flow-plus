{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "quotes": [2, "single"],
    "eol-last": [0],
    "no-underscore-dangle": [0],
    "no-cond-assign": 0,
    "indent": [2, 2],
    "semi": [2, "never"],
    "camelcase": 0,
    "comma-style": [2, "first"],
    "new-cap": 0,
    "strict": 0,
    "no-unused-expressions": 0,
    "semi-spacing": 0,
    "consistent-return": 2,
    "default-case": 2,
    "radix": 2,
    "brace-style": 2,
    "dot-location": [2, "property"]
  }
}
